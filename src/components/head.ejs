<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <%# Header Accept %>
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <meta http-equiv="accept-encoding" content="gzip, deflate">
    <meta http-equiv="timing-allow-origin" content="*">


    <title><%= title || process.env.NODE_APP_NAME %></title>
    <meta name="description" content="<%= description || process.env.NODE_APP_DESCRIPTION %>">
    
    <%# og %>
    <meta property="og:title" content="<%= title || process.env.NODE_APP_NAME %>" />
    <meta property="og:site_name" content="<%= process.env.NODE_APP_NAME %>" />
    <meta property="og:type" content="website" />
    <meta property="og:image" content="<%= imageUrl || process.env.NODE_LOGO_URL %>" />
    <meta property="og:url" content="<%= process.env.NODE_APP_URL %>" />
    <meta property="og:description" content="<%= description || process.env.NODE_APP_DESCRIPTION %>" />
    
    <%# twitter %>
    <meta name="twitter:title" content="<%= title || process.env.NODE_APP_NAME %>" />
    <meta name="twitter:description" content="<%= description || process.env.NODE_APP_DESCRIPTION %>" />
    <meta name="twitter:image" content="<%= imageUrl %>" />
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:site" content="https://twitter.com/" />
    
    <%# Ico for Browser Support %>
    <link rel="apple-touch-icon" sizes="180x180" href="assets/icons/logo/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="assets/icons/logo/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="assets/icons/logo/favicon-16x16.png">
    
    <%# Theme %>
    <meta name="theme-color" content="#ffffff"/>
    
    <%# Current Link %>
    <link rel="canonical" href="<%= process.env.NODE_APP_URL %>" />
    
    <%# human verify %>
    <meta name="robots" content="follow, index" />
    
    <link rel="manifest" href="static/site.webmanifest">
    
    <%# Style %>
    <link rel="stylesheet" href="css/app.css">
</head>
